{
  "name": "library-management-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Modern library management system frontend built with React and Vite",
  "author": "Pawan Fuke, Mayur Darji, Omkar Deshpande",
  "keywords": [
    "react",
    "vite",
    "library",
    "management",
    "frontend"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "terser": "^5.44.0",
    "vite": "^7.1.7"
  }
}
